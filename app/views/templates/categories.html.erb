<div class="content no-secondary-header" ng-controller="CategoryCtrl" style="padding-top: 50px;">
  <ul class="table-view" ng-if="categories">
    <li class="table-view-cell" ng-repeat="category in categories">
      <%= link_to '{{ category[1] }}', '#/category/{{ category[0] }}' %>
    </li>
  </ul>
</div>
